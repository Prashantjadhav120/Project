//Using Super Keyword
class Perent{
	
void m1(int a){
		System.out.println("Parent m1()"+a);}
}

class Super extends Perent {

	void m1(int a){System.out.println("child M1()"+a);}

    void m3(){
	this.m1(10);
	System.out.println("child2()");
	super.m1(100);
}
public static void main(String[] args) {

	Super s=new Super();
	s.m3();
	}
}